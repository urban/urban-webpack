    "babel-core": "*",
    "babel-runtime": "*",
    "babel-loader": "*",
    "css-loader": "*",
    "cssnext-loader": "*",
    "extract-text-webpack-plugin": "*",
    "file-loader": "*",
    "json-loader": "*",
    "react": "*",
    "react-hot-loader": "*",
    "style-loader": "*",
    "url-loader": "*",
    "webpack-dev-server": "*"
